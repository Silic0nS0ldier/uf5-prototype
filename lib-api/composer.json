    "homepage": "https://github.com/userfrosting/UserFrosting",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Weissman",
            "homepage": "https://alexanderweissman.com"
        },
        {
            "name": "Louis Charette",
            "homepage": "https://bbqsoftwares.com"
        },
        {
            "name": "Jordan Mele",
